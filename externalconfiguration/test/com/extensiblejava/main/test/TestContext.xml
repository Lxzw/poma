<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">

<beans>
	<bean id="firstBean" class="com.extensiblejava.main.FirstBean">
		<constructor-arg><ref bean="injectedBean"/></constructor-arg>
	</bean>
	
	<bean id="injectedBean" class="com.extensiblejava.main.test.MockInjection">
		<property name="uid" value="springuid"/>
		<property name="pwd" value="springpwd"/>
	</bean>
	
</beans>